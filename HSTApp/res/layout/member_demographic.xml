 <RelativeLayout
        android:id="@+id/layoutMemberDemoGraphic"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/white"
        android:typeface="sans"
        >

        
        

        <TextView
            android:id="@+id/personal_information"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="22dp"
            android:text="@string/personal_information"
            android:textColor="@color/black"
            android:textSize="15sp"
            android:typeface="sans"
             />

        <TextView
            android:id="@+id/member_age"
            android:layout_width="275dp"
            android:layout_height="35dp"
            android:layout_below="@+id/personal_information"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="15dp"
            android:layout_marginLeft="5dp"
            android:layout_marginRight="5dp"
            android:paddingLeft="5dp"
            android:text="@string/member_age" 
            android:background="@drawable/upper_textbox"
            android:textSize="20sp"
            android:gravity="center"
            android:typeface="sans"
            />
     

 <TextView
     android:id="@+id/member_gender"
     android:layout_width="275dp"
     android:layout_height="35dp"
     android:layout_alignLeft="@+id/member_age"
     android:layout_below="@+id/member_age"
     android:background="@drawable/lower_text_box"
     android:gravity="center"
     android:paddingLeft="5dp"
     android:text="@string/member_gender"
     android:textSize="20sp"
     android:typeface="sans"
      />

 <ImageView
     android:id="@+id/imageView1"
     android:layout_width="150dp"
     android:layout_height="150dp"
     android:layout_below="@+id/member_gender"
     android:layout_centerHorizontal="true"
     android:layout_marginTop="70dp"
     android:src="@drawable/progress" 
     android:typeface="sans"
     />

    </RelativeLayout>
</ScrollView>